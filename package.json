{
  "name": "cashdab-landing",
  "version": "1.0.0",
  "description": "캐시답 랜딩 웹페이지",
  "main": "tailwind.config.js",
  "scripts": {
    "start": "npx tailwindcss -i ./src/style.css -o ./public/style.css --watch",
    "build": "npx tailwindcss -i ./src/style.css -o ./public/style.css && cp -r src/imgs public && cp -f src/*.html public/ && sed -i '.bak' 's/\\/public\\/style.css/style.css/g' public/index.html && rm -f ./public/*.bak && cp -rf ./.well-known public/",
    "deploy": "firebase deploy",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bizcowork/cashdab-landing.git"
  },
  "keywords": [
    "캐시답",
    "설문"
  ],
  "author": "dev@bizcowork.co.kr",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/bizcowork/cashdab-landing/issues"
  },
  "homepage": "https://github.com/bizcowork/cashdab-landing/"
}
